<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Color x:Key="BackgroundBlack">#000000</Color>
    <Color x:Key="BackgroundGrey1">#171717</Color>
    <Color x:Key="BackgroundGrey2">#191919</Color>
    <Color x:Key="BackgroundGrey3">#1E1E1E</Color>
    <Color x:Key="BackgroundGrey4">#212121</Color>
    <Color x:Key="BackgroundGrey5">#292929</Color>

    <Color x:Key="HoverGrey">#292929</Color>
    <Color x:Key="Selected">#4E4E4E</Color>
    <Color x:Key="BorderBrush">#3C3C3C</Color>

    <Color x:Key="Icon">#9E9E9E</Color>

    <Color x:Key="FontWhite">#FFFFFF</Color>
    <Color x:Key="FontGreyWhite">#E0E0E0</Color>
    <Color x:Key="FontLightGrey">#BCBBBB</Color>
    <Color x:Key="FontGrey">#757575</Color>


    <SolidColorBrush Color="{StaticResource BackgroundBlack}" x:Key="BackgroundBlackBrush"/>
    <SolidColorBrush Color="{StaticResource BackgroundGrey1}" x:Key="BackgroundGrey1Brush"/>
    <SolidColorBrush Color="{StaticResource BackgroundGrey2}" x:Key="BackgroundGrey2Brush"/>
    <SolidColorBrush Color="{StaticResource BackgroundGrey3}" x:Key="BackgroundGrey3Brush"/>
    <SolidColorBrush Color="{StaticResource BackgroundGrey4}" x:Key="BackgroundGrey4Brush"/>
    <SolidColorBrush Color="{StaticResource BackgroundGrey5}" x:Key="BackgroundGrey5Brush"/>

    <SolidColorBrush Color="{StaticResource HoverGrey}" x:Key="HoverGreyBrush"/>
    <SolidColorBrush Color="{StaticResource Selected}" x:Key="SelectedBrush"/>
    <SolidColorBrush Color="{StaticResource BorderBrush}" x:Key="BorderBrushBrush"/>

    <SolidColorBrush Color="{StaticResource Icon}" x:Key="IconBrush"/>

    <SolidColorBrush Color="{StaticResource FontWhite}" x:Key="FontWhiteBrush"/>
    <SolidColorBrush Color="{StaticResource FontGreyWhite}" x:Key="FontGreyWhiteBrush"/>
    <SolidColorBrush Color="{StaticResource FontLightGrey}" x:Key="FontLightGreyBrush"/>
    <SolidColorBrush Color="{StaticResource FontGrey}" x:Key="FontGreyBrush"/>




    <Color x:Key="AlmostBlack">#141414</Color>
    <Color x:Key="VeryDarkGray">#121212</Color>
    <Color x:Key="DarkGray">#1e1e1e</Color>
    <Color x:Key="MidGray">#4A4A4A</Color>
    <Color x:Key="DeepTeal">#304047</Color>
    <Color x:Key="MutedRed">#CF694A</Color>
    <Color x:Key="DarkCyan">#007D90</Color>
    <Color x:Key="SlateBlue">#607D8B</Color>
    <Color x:Key="MediumGray">#757575</Color>
    <Color x:Key="SilverGray">#BDBDBD</Color>
    <Color x:Key="LightGray">#E0E0E0</Color>
    <Color x:Key="SoftYellow">#F9EE9A</Color>
    <Color x:Key="VibrantBlue">#2196F3</Color>


    <SolidColorBrush Color="{StaticResource AlmostBlack}" x:Key="AlmostBlackBrush"/>
    <SolidColorBrush Color="{StaticResource VeryDarkGray}" x:Key="VeryDarkGrayBrush"/>
    <SolidColorBrush Color="{StaticResource DarkGray}" x:Key="DarkGrayBrush"/>
    <SolidColorBrush Color="{StaticResource MidGray}" x:Key="MidGrayBrush"/>
    <SolidColorBrush Color="{StaticResource DeepTeal}" x:Key="DeepTealBrush"/>
    <SolidColorBrush Color="{StaticResource MutedRed}" x:Key="MutedRedBrush"/>
    <SolidColorBrush Color="{StaticResource DarkCyan}" x:Key="DarkCyanBrush"/>
    <SolidColorBrush Color="{StaticResource SlateBlue}" x:Key="SlateBlueBrush"/>
    <SolidColorBrush Color="{StaticResource MediumGray}" x:Key="MediumGrayBrush"/>
    <SolidColorBrush Color="{StaticResource SilverGray}" x:Key="SilverGrayBrush"/>
    <SolidColorBrush Color="{StaticResource LightGray}" x:Key="LightGrayBrush"/>
    <SolidColorBrush Color="{StaticResource SoftYellow}" x:Key="SoftYellowBrush"/>
    <SolidColorBrush Color="{StaticResource VibrantBlue}" x:Key="VibrantBlueBrush"/>
</ResourceDictionary>